    <body>
      <header class="cd-main-header">
	    	<a href="#0" class="cd-logo"><img src="/images/cd-logo.svg" alt="Logo"></a>
    
	    	<a href="#0" class="cd-nav-trigger">Menu<span></span></a>
    
	    	<nav class="cd-nav">
	    		<ul class="cd-top-nav">
	    			
                            <li><a href="/login">Login</a></li>
                            <li><a href="/register">Sign Up</a></li>
                        
                            <li><a href="#">Signed In As ChrisDummy</a></li>
                            <li><a href="/logout">Logout</a></li>
                        
	    		</ul>
	    	</nav>
	    </header> <!-- .cd-main-header -->
	    <main class="cd-main-content">
	      <nav class="cd-side-nav">
	      	<ul>      
	      	 		<li class="cd-label">Views</li>
	      	      <li class="has-children">
	      				  <a class="view" href="/feeds/">All Feeds</a>
	      				</li>
	      			</li>
	      			<li class="cd-label">Feeds</li>
	      	      <% feeds.forEach(function(feed){ %>
	      	      <li data-id="<%= feed._id %>" class="has-children <%= feed.feedTitle %>">
	      				  <a class="feeds" href="/feeds/<%= feed._id %>"><%= feed.feedTitle %></a>
	      				</li>
	      				<% }); %>
	      			</li>
	      			<li class="sidebar-button">
	      			  <button id="feedRefresh" class="btn btn-primary"><i class="fas fa-sync"></i> Refresh</button>
	      			</li>
	      	</ul>
	      	<ul>
                  <li class="cd-label">Add new Feeds</li>
                  <li>
                    <form class="addFeed" action="/feeds" method="POST">
                      <div class="form-group">
                        <!-- <label for="feedUrl">Add new feed</label> -->
                        <input type="text" class="form-control" id="feedUrl" name="feedUrl" aria  -describedby                ="emailHelp" placeholder="Feed URL">
                      </div>
                      <button type="submit" class="btn btn-primary">Add Feed</button>
                    </form>
                  </li>
              </ul>
	      </nav>